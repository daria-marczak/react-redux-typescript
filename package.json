  "scripts": {
    "prod:styled": "webpack --config webpack.config.js --env.mode production --env.styling=styled",
    "prod:scss": "webpack --config webpack.config.js --env.mode production --env.styling=scss",
    "dev:styled": "webpack-dev-server --hot=only --env.mode development --env.styling=styled",
    "dev:scss": "webpack-dev-server --hot=only --env.mode development --env.styling=scss",
    "format": "prettier --write ./src/**/*.tsx",
    "lint": "eslint src/**/*.tsx",
    "lint:fix": "eslint --write src/**/*.tsx",
    "test": "jest",
    "deploy": "yarn run prod:scss && aws s3 cp dist/ s3://dmariatesting.com --recursive",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
